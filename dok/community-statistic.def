CREATE TABLE chat (
  c_timestamp char(16) DEFAULT '' NOT NULL,
  c_users     int(11) DEFAULT '0' NOT NULL,
  c_host      char(255) DEFAULT '' NOT NULL,
  KEY c_timestamp (c_timestamp),
  KEY c_users (c_users),
	KEY c_host (c_host)
);
